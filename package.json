{
  "name": "api-template",
  "version": "1.0.0",
  "description": "API Template project",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dral/api-template"
  },
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "prod": "NODE_ENV=production node dist/src/index.js",
    "start": "ts-node src/index.ts",
    "watch": "nodemon",
    "build": "tsc --project tsconfig.production.json",
    "clean": "tsc --build --clean",
    "tsc": "tsc",
    "test": "ts-node ./node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "ts-node ./node_modules/jest/bin/jest.js --watch --detectOpenHandles src",
    "lint": "eslint .",
    "license:check": "license-checker --production --excludePrivatePackages --onlyAllow 'MIT;ISC;0BSD;BSD-2-Clause;BSD-3-Clause;CC-BY-3.0;CC0-1.0;Apache-2.0;Python-2.0' --summary",
    "license:report": "license-checker --production --excludePrivatePackages --relativeLicensePath --json --out licenses.json",
    "dumpApiSpec": "ts-node src/base/dumpOpenApi.ts"
  },
  "author": "Draltan MARIN <moi@draltan.com>",
  "license": "ISC",
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^2.3.1",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/license-checker": "^25.0.3",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "mongodb-memory-server": "^8.12.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "license-checker": "^25.0.1",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  }
}
